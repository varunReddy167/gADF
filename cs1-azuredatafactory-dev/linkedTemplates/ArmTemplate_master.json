{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "cs1-azuredatafactory-dev"
		},
		"AzureDataLakeStorage_sink_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage_sink'"
		},
		"ls_azuresqlda_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_azuresqlda'"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://csadlsgen2accountdev.dfs.core.windows.net/"
		},
		"AzureDataLakeStorage1_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "f365e77f-2d81-4093-a9c6-989e5d3b8cd7"
		},
		"AzureDataLakeStorage1_properties_typeProperties_servicePrincipalId": {
			"type": "string",
			"defaultValue": "dcb06972-306b-4815-9fb9-cb69131f3ae7"
		},
		"AzureDataLakeStorage_sink_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://csadlsgen2accountdev.dfs.core.windows.net/"
		},
		"AzureKeyVault1_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://cs-keyvault24-dev.vault.azure.net/"
		},
		"Rest_api_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://airlabs.co/api/v9/airlines"
		},
		"dev_ls_adlsgen2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://devadlssink.dfs.core.windows.net/"
		},
		"dev_ls_adlsgen2_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "f365e77f-2d81-4093-a9c6-989e5d3b8cd7"
		},
		"dev_ls_adlsgen2_properties_typeProperties_servicePrincipalId": {
			"type": "string",
			"defaultValue": "dcb06972-306b-4815-9fb9-cb69131f3ae7"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "cs1-azuredatafactory-dev_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage_sink_accountKey": {
						"value": "[parameters('AzureDataLakeStorage_sink_accountKey')]"
					},
					"ls_azuresqlda_connectionString": {
						"value": "[parameters('ls_azuresqlda_connectionString')]"
					},
					"AzureDataLakeStorage_sink_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage_sink_properties_typeProperties_url')]"
					},
					"AzureKeyVault1_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault1_properties_typeProperties_baseUrl')]"
					},
					"Rest_api_properties_typeProperties_url": {
						"value": "[parameters('Rest_api_properties_typeProperties_url')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_tenant": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_tenant')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_servicePrincipalId": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_servicePrincipalId')]"
					},
					"dev_ls_adlsgen2_properties_typeProperties_url": {
						"value": "[parameters('dev_ls_adlsgen2_properties_typeProperties_url')]"
					},
					"dev_ls_adlsgen2_properties_typeProperties_tenant": {
						"value": "[parameters('dev_ls_adlsgen2_properties_typeProperties_tenant')]"
					},
					"dev_ls_adlsgen2_properties_typeProperties_servicePrincipalId": {
						"value": "[parameters('dev_ls_adlsgen2_properties_typeProperties_servicePrincipalId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}